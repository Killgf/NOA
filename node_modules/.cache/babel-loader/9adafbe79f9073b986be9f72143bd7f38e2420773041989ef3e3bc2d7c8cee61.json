{"ast":null,"code":"import GallerySlideshow from 'vue-gallery-slideshow';\nexport default {\n  components: {\n    GallerySlideshow\n  },\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      time: {\n        days: 0,\n        hours: 0,\n        minutes: 0,\n        seconds: 0\n      },\n      photos: [{\n        id: 1,\n        url: 'path-to-your-photo-1.jpg'\n      }],\n      images: []\n    };\n  },\n  methods: {\n    getTimeRemaining(endtime) {\n      const t = Date.parse(endtime) - Date.now();\n      const seconds = Math.floor(t / 1000 % 60);\n      const minutes = Math.floor(t / 1000 / 60 % 60);\n      const hours = Math.floor(t / (1000 * 60 * 60) % 24);\n      const days = Math.floor(t / (1000 * 60 * 60 * 24));\n      return {\n        total: t,\n        days: days,\n        hours: hours,\n        minutes: minutes,\n        seconds: seconds\n      };\n    },\n    updateClock(endtime) {\n      const t = this.getTimeRemaining(endtime);\n      this.time.days = t.days;\n      this.time.hours = t.hours;\n      this.time.minutes = t.minutes;\n      this.time.seconds = t.seconds;\n      if (t.total <= 0) {\n        clearInterval(this.timeinterval);\n      }\n    },\n    initializeClock(id, endtime) {\n      this.updateClock(endtime);\n      this.timeinterval = setInterval(() => this.updateClock(endtime), 1000);\n    },\n    openSlideshow(photo) {\n      this.images = [{\n        src: photo.url\n      }];\n    },\n    closeSlideshow() {\n      this.images = [];\n    }\n  },\n  mounted() {\n    const deadline = new Date('August 22, 2023 00:00:00 GMT+0000');\n    this.initializeClock('countdown', deadline);\n  },\n  beforeDestroy() {\n    clearInterval(this.timeinterval);\n  }\n};","map":{"version":3,"names":["GallerySlideshow","components","name","props","msg","String","data","time","days","hours","minutes","seconds","photos","id","url","images","methods","getTimeRemaining","endtime","t","Date","parse","now","Math","floor","total","updateClock","clearInterval","timeinterval","initializeClock","setInterval","openSlideshow","photo","src","closeSlideshow","mounted","deadline","beforeDestroy"],"sources":["C:\\Killgf\\Hogwards\\best-team\\src\\components\\HelloWorld.vue"],"sourcesContent":["<template>\n    <div class=\"wrapper\">\n        <div class=\"container\">\n            <header class=\"header\">\n                <div class=\"header__logo\">\n                    <a href=\"/home\"><img src=\"..//assets/img/logo_one-1.svg\" alt=\"\"></a>\n                </div>\n                <div class=\"header__title\">BEST OF THE BEST</div>\n                <button class=\"header__btn-1\">Click</button>\n            </header>\n            <div class=\"main\">\n                <div class=\"slider-container\">\n                    <div>\n                        <gallery-slideshow :images=\"images\" @close=\"closeSlideshow\" />\n                        <div v-for=\"photo in photos\" :key=\"photo.id\" @click=\"openSlideshow(photo)\">\n                            <img src=\"../assets/img/1.jpg\" alt=\"\">\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"timer\">\n                <h1 class=\"countdown-title\">ТУСОВКА ЧЕРЕЗ:</h1>\n                <div id=\"countdown\" class=\"countdown\">\n                    <div class=\"countdown-number\">\n                        <span class=\"days countdown-time\">{{ time.days }}</span>\n                        <span class=\"countdown-text\">Дні</span>\n                    </div>\n                    <div class=\"countdown-number\">\n                        <span class=\"hours countdown-time\">{{ ('0' + time.hours).slice(-2) }}</span>\n                        <span class=\"countdown-text\">Години</span>\n                    </div>\n                    <div class=\"countdown-number\">\n                        <span class=\"minutes countdown-time\">{{ ('0' + time.minutes).slice(-2) }}</span>\n                        <span class=\"countdown-text\">Хвилини</span>\n                    </div>\n                    <div class=\"countdown-number\">\n                        <span class=\"seconds countdown-time\">{{ ('0' + time.seconds).slice(-2) }}</span>\n                        <span class=\"countdown-text\">Секунди</span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import GallerySlideshow from 'vue-gallery-slideshow';\n\n    export default {\n        components: {\n            GallerySlideshow\n        },\n        name: 'HelloWorld',\n        props: {\n            msg: String\n        },\n        data() {\n            return {\n                time: {\n                    days: 0,\n                    hours: 0,\n                    minutes: 0,\n                    seconds: 0\n                },\n                photos: [\n                    { id: 1, url: 'path-to-your-photo-1.jpg' },\n                ],\n                images: []\n            }\n        },\n\n        methods: {\n            getTimeRemaining(endtime) {\n                const t = Date.parse(endtime) - Date.now();\n                const seconds = Math.floor((t / 1000) % 60);\n                const minutes = Math.floor((t / 1000 / 60) % 60);\n                const hours = Math.floor((t / (1000 * 60 * 60)) % 24);\n                const days = Math.floor(t / (1000 * 60 * 60 * 24));\n                return {\n                    total: t,\n                    days: days,\n                    hours: hours,\n                    minutes: minutes,\n                    seconds: seconds\n                };\n            },\n            updateClock(endtime) {\n                const t = this.getTimeRemaining(endtime);\n\n                this.time.days = t.days;\n                this.time.hours = t.hours;\n                this.time.minutes = t.minutes;\n                this.time.seconds = t.seconds;\n\n                if (t.total <= 0) {\n                    clearInterval(this.timeinterval);\n                }\n            },\n            initializeClock(id, endtime) {\n                this.updateClock(endtime);\n                this.timeinterval = setInterval(() => this.updateClock(endtime), 1000);\n            },\n            openSlideshow(photo) {\n                this.images = [{ src: photo.url }];\n            },\n            closeSlideshow() {\n                this.images = [];\n            }\n        },\n        mounted() {\n            const deadline = new Date('August 22, 2023 00:00:00 GMT+0000');\n            this.initializeClock('countdown', deadline);\n        },\n        beforeDestroy() {\n            clearInterval(this.timeinterval);\n        }\n    };\n</script>\n"],"mappings":"AA8CI,OAAOA,gBAAe,MAAO,uBAAuB;AAEpD,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EACDE,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACHC,GAAG,EAAEC;EACT,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFC,IAAI,EAAE,CAAC;QACPC,KAAK,EAAE,CAAC;QACRC,OAAO,EAAE,CAAC;QACVC,OAAO,EAAE;MACb,CAAC;MACDC,MAAM,EAAE,CACJ;QAAEC,EAAE,EAAE,CAAC;QAAEC,GAAG,EAAE;MAA2B,CAAC,CAC7C;MACDC,MAAM,EAAE;IACZ;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,gBAAgBA,CAACC,OAAO,EAAE;MACtB,MAAMC,CAAA,GAAIC,IAAI,CAACC,KAAK,CAACH,OAAO,IAAIE,IAAI,CAACE,GAAG,CAAC,CAAC;MAC1C,MAAMX,OAAM,GAAIY,IAAI,CAACC,KAAK,CAAEL,CAAA,GAAI,IAAI,GAAI,EAAE,CAAC;MAC3C,MAAMT,OAAM,GAAIa,IAAI,CAACC,KAAK,CAAEL,CAAA,GAAI,IAAG,GAAI,EAAE,GAAI,EAAE,CAAC;MAChD,MAAMV,KAAI,GAAIc,IAAI,CAACC,KAAK,CAAEL,CAAA,IAAK,IAAG,GAAI,EAAC,GAAI,EAAE,CAAC,GAAI,EAAE,CAAC;MACrD,MAAMX,IAAG,GAAIe,IAAI,CAACC,KAAK,CAACL,CAAA,IAAK,IAAG,GAAI,EAAC,GAAI,EAAC,GAAI,EAAE,CAAC,CAAC;MAClD,OAAO;QACHM,KAAK,EAAEN,CAAC;QACRX,IAAI,EAAEA,IAAI;QACVC,KAAK,EAAEA,KAAK;QACZC,OAAO,EAAEA,OAAO;QAChBC,OAAO,EAAEA;MACb,CAAC;IACL,CAAC;IACDe,WAAWA,CAACR,OAAO,EAAE;MACjB,MAAMC,CAAA,GAAI,IAAI,CAACF,gBAAgB,CAACC,OAAO,CAAC;MAExC,IAAI,CAACX,IAAI,CAACC,IAAG,GAAIW,CAAC,CAACX,IAAI;MACvB,IAAI,CAACD,IAAI,CAACE,KAAI,GAAIU,CAAC,CAACV,KAAK;MACzB,IAAI,CAACF,IA